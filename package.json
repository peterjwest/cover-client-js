{
  "name": "diffblue-cover-client-js",
  "version": "0.0.1",
  "description": "Diffblue Cover Client JS - Helper for integration with Cover",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:diffblue/diffblue-cover-client-js.git"
  },
  "author": "DiffBlue Limited",
  "bugs": {
    "url": "https://github.com/diffblue/diffblue-cover-client-js/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/diffblue/diffblue-cover-client-js#readme",
  "scripts": {
    "coverage": "rm -rf coverage && NODE_DEBUG=nyc nyc --check-coverage --lines 100 mocha -require ts-node/register 'tests/unit/**/*.ts'",
    "test": "mocha --require ts-node/register 'tests/unit/**/*.ts'"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/mocha": "5.2.6",
    "mocha": "6.1.3",
    "nyc": "13.3.0",
    "ts-node": "8.0.3"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "node_modules/**",
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "all": true,
    "reporter": [
      "text",
      "text-summary",
      "lcov"
    ]
  }
}
